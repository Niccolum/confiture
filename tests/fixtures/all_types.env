# Comprehensive test file with all Python basic types
# All values are strings in ENV, will be converted by adaptix
# Complex structures use JSON format: [1,2,3] or {"key":"value"}

# SCALAR TYPES
STRING_VALUE=hello world
INTEGER_VALUE=42
FLOAT_VALUE=3.14159
BOOLEAN_VALUE=true
NONE_VALUE=

# LISTS (JSON format)
LIST_STRINGS=["item1","item2","item3"]
LIST_INTEGERS=[1,2,3,4,5]
LIST_MIXED=["string",42,3.14,true,null]
LIST_NESTED=[["a","b"],["c","d"]]
LIST_DICTS=[{"name":"Alice","age":30},{"name":"Bob","age":25}]

# TUPLES (JSON arrays, will be converted to tuples)
TUPLE_SIMPLE=[1,2,3]
TUPLE_MIXED=["text",42,true]
TUPLE_NESTED=[[1,2,3],["a","b","c"]]

# SETS (JSON arrays, will be converted to sets)
SET_INTEGERS=[1,2,3,4,5]
SET_STRINGS=["apple","banana","cherry"]

# DICTIONARIES (__ notation for simple dicts, or JSON for complex)
DICT_SIMPLE__KEY1=value1
DICT_SIMPLE__KEY2=value2

DICT_MIXED__STRING=text
DICT_MIXED__NUMBER=42
DICT_MIXED__FLOAT=3.14
DICT_MIXED__BOOL=true
DICT_MIXED__LIST=[1,2,3]

DICT_NESTED__LEVEL1__LEVEL2__LEVEL3=deep_value

DICT_INT_KEYS__1=one
DICT_INT_KEYS__2=two
DICT_INT_KEYS__3=three

DICT_LIST_DICT={"users":[{"name":"Alice","role":"admin"},{"name":"Bob","role":"user"}],"teams":[{"name":"backend","size":5}]}

# SPECIAL TYPES (as strings)
DATE_VALUE=2024-01-15
DATETIME_VALUE=2024-01-15T10:30:00
DATETIME_VALUE_WITH_TIMEZONE=2024-01-15T10:30:00+03:00
DATETIME_VALUE_WITH_Z_TIMEZONE=2024-01-15T10:30:00Z
TIME_VALUE=10:30:00
BYTES_VALUE=binary data
BYTEARRAY_VALUE=binary
COMPLEX_VALUE=1+2j

# EDGE CASES
EMPTY_STRING=
EMPTY_LIST=[]
EMPTY_DICT={}
ZERO_INT=0
ZERO_FLOAT=0.0
FALSE_BOOL=false

# ADVANCED PATTERNS
OPTIONAL_STRING=
UNION_TYPE=42
NESTED_OPTIONAL=[{"name":"Alice","email":"alice@example.com"},{"name":"Bob","email":null}]

# RANGE (JSON array)
RANGE_VALUES=[0,2,4,6,8]

# FROZENSET (JSON array, will be converted to frozenset)
FROZENSET_VALUE=[1,2,3,4,5]
